<p id="notice"><%= notice %></p>

<div class="container">
<h2>Listing Users</h2>

  <div class="panel panel-default">
    <table class="table">
      <thead>
        <tr>
          <th>Username</th>
          <th>Email</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @users.each do |user| %>
          <tr>
            <td><%= link_to user.username, user %></td>
            <td><%= user.email %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
<%= link_to 'New User', new_user_path %>

</div>
<br>

